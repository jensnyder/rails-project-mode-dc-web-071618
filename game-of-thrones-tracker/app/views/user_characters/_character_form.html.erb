<%= form_for @user_character do |f| %>
  <%= f.label :character %>
  <%= f.collection_select :character_id, Character.all, :id, :name %>
  <%= f.label :user %>
  <%= f.collection_select :user_id, User.all, :id, :username %>
  <%= f.label :status %>
  <%= f.select(:status, ["unknown", "alive", "dead"]) %>
  <%= f.label :note %>
  <%= f.text_area :note %>
  <%= f.submit %>
<% end %>
